<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.live.communityskyeye">

    <application
        android:name=".MyOwnApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SceneActivity"
            android:label="@string/scene_activity"
            android:launchMode="singleTask" />
        <activity
            android:name=".add_surface"
            android:label="@string/title_activity_add_surface"
            android:launchMode="standard" />
        <activity
            android:name=".SubScene"
            android:label="@string/title_activity_sub_scene"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>